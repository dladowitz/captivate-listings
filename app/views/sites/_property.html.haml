                  - if @property.garden == "yes"
                    .col.sqs-col-2.span-2.home-icon-div
                      #block-yui_3_17_2_30_1441942307387_44722.sqs-block.image-block.sqs-block-image{"data-block-type" => "5"}
                        .sqs-block-content
                          .image-block-outer-wrapper.layout-caption-below
