                  #my-iframe
                    / Matterport Embed URL
                    %iframe{:allowfullscreen => "", "data-embed" => "true", :frameborder => "0", :height => "584", :src => "#{@property.matterport_url}", :width => "1050"}

            / Agent Detals Area
